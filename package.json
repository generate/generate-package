{
  "name": "generate-package",
  "description": "Generate a package.json from a pre-defined or user-defined template. This generator can be used from the command line when globally installed, or as a plugin or sub-generator in your own generator.",
  "version": "0.7.0",
  "homepage": "https://github.com/generate/generate-package",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "contributors": [
    "Dawson Botsford <dawsonbotsford@gmail.com> (http://dawsonbotsford.com)",
    "Jon Schlinkert <jon.schlinkert@sellside.com> (http://twitter.com/jonschlinkert)"
  ],
  "repository": "generate/generate-package",
  "bugs": {
    "url": "https://github.com/generate/generate-package/issues"
  },
  "license": "MIT",
  "files": [
    "generator.js",
    "index.js",
    "templates",
    "utils.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "generate-defaults": "^0.6.5",
    "gulp-choose-files": "^0.1.3",
    "gulp-normalize-pkg": "^0.1.2",
    "gulp-pick-keys": "^0.1.1",
    "helper-date": "^0.2.2",
    "is-valid-app": "^0.2.1",
    "lazy-cache": "^2.0.1"
  },
  "devDependencies": {
    "delete": "^0.3.2",
    "fs-exists-sync": "^0.1.0",
    "generate": "^0.13.1",
    "global-modules": "^0.2.3",
    "gulp-format-md": "^0.1.11",
    "mocha": "^3.1.0"
  },
  "keywords": [
    "app",
    "boilerplate",
    "build",
    "cli",
    "cli-app",
    "command-line",
    "create",
    "dev",
    "development",
    "framework",
    "front",
    "frontend",
    "generate",
    "generate-generator",
    "generate-plugin",
    "generategenerator",
    "generateplugin",
    "generator",
    "init",
    "initialize",
    "json",
    "new",
    "package",
    "package-json",
    "pkg",
    "plugin",
    "project",
    "projects",
    "scaffold",
    "scaffolder",
    "scaffolding",
    "template",
    "templates",
    "tool",
    "web",
    "webapp",
    "yeoman",
    "yo"
  ],
  "verb": {
    "toc": {
      "method": "preWrite"
    },
    "layout": "generator",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "generate-gitignore",
        "generate-license",
        "generate-readme"
      ]
    },
    "reflinks": [
      "assemble",
      "base",
      "generate",
      "generate-dest",
      "gulp",
      "normalize-pkg",
      "verb",
      "verb-readme-generator"
    ],
    "lint": {
      "reflinks": true
    }
  }
