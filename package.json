{
  "name": "generate-package",
  "description": "Generate a package.json for a project. This generator can be used as a plugin or sub-generator in your own generator, as a component of a larger build workflow.",
  "version": "0.2.0",
  "homepage": "https://github.com/generate/generate-package",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "generate/generate-package",
  "bugs": {
    "url": "https://github.com/jonschlinkert/generate-package/issues"
  },
  "license": "MIT",
  "files": [
    "generator.js",
    "index.js",
    "LICENSE",
    "README.md",
    "templates",
    "utils.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "common-questions": "^0.2.4",
    "generate-collections": "^0.3.4",
    "generate-defaults": "^0.3.0",
    "gulp-choose-files": "^0.1.1",
    "gulp-normalize-pkg": "^0.1.1",
    "gulp-pick-keys": "^0.1.1",
    "is-valid-app": "^0.1.1",
    "lazy-cache": "^2.0.1"
  },
  "devDependencies": {
    "delete": "^0.3.2",
    "fs-exists-sync": "^0.1.0",
    "generate": "^0.7.1",
    "global-modules": "^0.2.2",
    "gulp-format-md": "^0.1.9",
    "mocha": "^2.5.3"
  },
  "keywords": [
    "build",
    "cli",
    "create",
    "dev",
    "development",
    "generate",
    "generateplugin",
    "generator",
    "init",
    "initialize",
    "json",
    "new",
    "package",
    "package-json",
    "pkg",
    "plugin",
    "project",
    "projects",
    "scaffold",
    "scaffolder",
    "scaffolding",
    "template",
    "templates",
    "yeoman",
    "yo"
  ],
  "verb": {
    "toc": false,
    "layout": "generator",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "generate",
        "generate-dest",
        "generate-file",
        "generate-git"
      ]
    },
    "reflinks": [
      "generate",
      "generate-dest",
      "normalize-pkg",
      "verb",
      "verb-readme-generator"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
